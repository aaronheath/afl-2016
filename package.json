{
  "name": "afl-2016",
  "version": "1.0.0",
  "scripts": {
    "start": "concurrently \"npm run tsc\" \"npm run templates\" \"npm run data\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "typings": "typings",
    "postinstall": "typings install",
    "templates": "rsync -a --prune-empty-dirs --include '*/' --include '*.html' --exclude '*' src/app/ public/app/",
    "data": "node src/data/main.js",
    "lint": "tslint src/app/**/*.ts"
  },
  "license": "ISC",
  "dependencies": {
    "angular2": "2.0.0-beta.9",
    "systemjs": "0.19.24",
    "semantic-ui": "^2.1.8",
    "lodash": "^4.6.1",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "zone.js": "^0.5.15"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "lite-server": "^2.1.0",
    "typescript": "^1.8.9",
    "typings": "^0.7.9",
    "tslint": "^3.6.0"
  }
}
